/**app.wxss**/
page,view,text{
  box-sizing: border-box;
}
button {
  background: initial;
}

button:focus{
  outline: 0;
}

button::after{
  border: none;
}


page {
  background: #fff;
}
.btn{
  width:670rpx;
  height:100rpx;
  background:rgba(255,65,14,1);
  box-shadow:0px 4rpx 14rpx 0px rgba(255,65,14,0.29);
  border-radius:7rpx;
  font-size:32rpx;
  font-family:PingFangSC-Semibold,PingFang SC;
  font-weight:600;
  color:rgba(255,255,255,1);
  line-height:100rpx;
  text-align: center;
}
.mt20{
  margin-top: 20rpx;
}